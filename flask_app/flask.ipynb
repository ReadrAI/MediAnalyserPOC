    "def my_threaded_func(arg, arg2):\n",
    "    print(\"Running thread! Args:\", (arg, arg2))\n",
    "    time.sleep(10)\n",
    "    print(\"Done!\")\n",
    "\n",
    "thread = threading.Thread(target=my_threaded_func, args=(\"I'ma\", \"thread\"))\n",
    "thread.start()\n",
    "print(\"Spun off thread\")"
